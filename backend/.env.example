PORT=5000
NODE_ENV=development

# Database (Not used in demo - using demo data)
# MONGODB_URI=mongodb://localhost:27017/ai-manager-demo

# JWT Secret (Not used in demo)
# JWT_SECRET=your-jwt-secret-key

# API Configuration
API_VERSION=v1
CORS_ORIGIN=http://localhost:3000

# Demo Mode Settings
DEMO_MODE=true
DEMO_DATA_SOURCE=local

# AI Configuration (Optional - will use demo mode if not provided)
# Get your API key from: https://platform.openai.com/api-keys
# OPENAI_API_KEY=your_openai_api_key_here

# Logging
LOG_LEVEL=info